{
  "extends": ["eslint:recommended", "google", "next", "next/core-web-vitals", "prettier"],
  "parser": "@typescript-eslint/parser", // TypeScriptを使用している場合
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error", 
    "require-jsdoc": "off"
  }
}